 declare module 'electron-shell' {

  declare type ApplicationConfig = {
    app: {
      name: string,
      version: string
    },
    host: string,
    platform: string,
    defaultLocale: string,
    user: string,
    paths: {
      appPath: string,
      home: string,
      temp: string,
      data: string
    }
  }

  declare interface IDocumentDatabase {
    save (doc:Object): Promise<*>;
    get (id:string): Promise<*>;
    query (view:string, options:Object): Promise<*>;
    bulkInsert (docs:Array<Object>) : Promise<*>;
  }

  declare interface ISqlDatabase {

  }

  declare interface ITripleStore {

  }

  declare interface IFileStorage {
    appFolder: string;
    baseFolder: string;
    iterate(path:string, pattern:string): Promise<*>;
  }

  declare interface IActivityService {
    getByType (type:string, docDB:IDocumentDatabase): void;
    getByDateRange (from:Date, to:Date, docDB:IDocumentDatabase): void;
    getByIssuer (issuer:string, docDB:IDocumentDatabase): void;
    find (filter:Object, docDB:IDocumentDatabase): void;
    create (event:Object, docDB:IDocumentDatabase): void;
  }

  declare interface IExtensionManager {
    mountAll (fileStorage:IFileStorage, docDB:IDocumentDatabase): void;
    activate (extension:string, docDB:IDocumentDatabase): void;
    deactivate (extension:string, docDB:IDocumentDatabase): void;
    install (extension:string, fileStorage:IFileStorage, docDB:IDocumentDatabase): void;
    uninstall (extension:string, fileStorage:IFileStorage, docDB:IDocumentDatabase): void;
  }

  declare interface ISettingManager {
    getAll (docDB:IDocumentDatabase): void;
    getByExtension (extension:string, docDB:IDocumentDatabase): void;
    update (setting:Object, docDB:IDocumentDatabase): void;
    import (settings:Array<Object>, docDB:IDocumentDatabase): void;
    export (docDB:IDocumentDatabase): void;
  }

  declare interface ITanslationManager {
    switchLocale (locale:string, docDB:IDocumentDatabase): void;
    update (locale:string, message:Object, docDB:IDocumentDatabase): void;
    import (locale:string, messages:Array<Object>, docDB:IDocumentDatabase): void;
    export (locale:string, docDB:IDocumentDatabase): void;
  }
}
